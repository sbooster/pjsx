{
  "name": "jsx",
  "sideEffects": false,
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": ["./dist/index.d.ts"],
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
